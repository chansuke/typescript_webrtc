{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "sourceMap": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "pretty": true,
    "removeComments": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "baseUrl": "web/static/js",
    "outDir": "ts-build",
    "allowJs": true
  },
  "exclude": [
      "node_modules",
      "priv",
      "ts-build"
  ]
}
